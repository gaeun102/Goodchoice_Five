<!-- ==== #29. mapper 기본설정 ==== -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="reservation">
		 
	<insert id="insertReservationInfo" parameterType="HashMap">
		insert into tbl_reservation(RESERV_ID, FK_ROOM_ID,FK_MEMBER_ID,CHECK_OUT_DATE,CHECK_IN_DATE,PRICE,RESERV_PERSON) 
		values(${reserve_id}, '25CQL548', ${member_id}, ${checkOut}, ${checkIn}, ${totalPrice}, ${userName})
	</insert>
		
		
		
</mapper>
